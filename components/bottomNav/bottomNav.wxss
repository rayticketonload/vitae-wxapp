@charset 'UTF-8';.bottomNavWrap{box-sizing:border-box;position:fixed;bottom:20rpx;left:20rpx}.bottomNavWrap .navBtn{box-sizing:border-box;display:inline-block;height:80rpx;line-height:80rpx;text-align:center;padding:0 20rpx;background-color:#262f3b;opacity:.9;transition-property:all;transition-duration:.3s;transition-timing-function:ease;transition-delay:0}.bottomNavWrap .navBtn:active{opacity:1}.bottomNavWrap .navBtn>image{width:40rpx;height:40rpx;display:inline-block;vertical-align:middle;margin-right:10rpx}.bottomNavWrap .navBtn>span{color:#fff;vertical-align:middle;font-size:28rpx}.bottomNavWrap .navBtn.addItem{border-top-left-radius:15rpx;border-top-right-radius:0;border-bottom-right-radius:0;border-bottom-left-radius:15rpx;border-right:1px solid #777}.bottomNavWrap .navBtn.addBox{border-top-left-radius:0;border-top-right-radius:15rpx;border-bottom-right-radius:15rpx;border-bottom-left-radius:0}.bottomNavWrap .navBtn.addBox>image{width:60rpx;height:60rpx;margin-top:-7rpx}.bottomNavWrap .navBtn.toIndex{border-radius:15rpx;width:80rpx;height:80rpx;text-align:center;margin-right:10rpx;padding:0}.bottomNavWrap .navBtn.toIndex>image{width:48rpx;height:48rpx;margin:-8rpx 0 0 0}.bottomNavWrap .navBtn.toMsg{border-radius:15rpx;width:80rpx;height:80rpx;text-align:center;margin-right:10rpx;padding:0;position:relative}.bottomNavWrap .navBtn.toMsg .msgDot{display:none;position:absolute;top:-10rpx;right:-10rpx;width:30rpx;height:30rpx;background-color:#fb6a01;border-radius:50%;animation-name:dot;animation-duration:1.6s;animation-timing-function:ease;animation-delay:0;animation-iteration-count:infinite;animation-direction:normal;animation-play-state:running;animation-fill-mode:both}@keyframes dot{0%{box-shadow:0 0 0 0 rgba(255,195,183,0.7)}100%{box-shadow:0 0 0 50rpx rgba(255,195,183,0)}}.bottomNavWrap .navBtn.toMsg.haveNewMsg .msgDot{display:block}.bottomNavWrap .navBtn.toMsg>image{width:48rpx;height:48rpx;margin:-8rpx 0 0 0}