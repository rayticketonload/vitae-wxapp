<view class="defaultFormWrap">
  <form bindsubmit="formSubmit" bindreset="formReset">
    <view class="formControlWrap fn-mb-20">
      <input name="boxName" placeholder="收纳点名称是？" placeholder-class="defaultPlaceHolderColor" value="" class="formControl" />
    </view>
    <view class="formControlWrap fn-mb-20">
      <pickerDAT>
        <view class="picker formControl">
          过期时间:  {{dateTimeArray[0][dateTime[0]]}}-{{dateTimeArray[1][dateTime[1]]}}-{{dateTimeArray[2][dateTime[2]]}} {{dateTimeArray[3][dateTime[3]]}}:{{dateTimeArray[4][dateTime[4]]}}:{{dateTimeArray[5][dateTime[5]]}}
        </view>
      </pickerDAT>
    </view>
    <view class="formControlWrap options fn-mb-60">
      <input name="boxLocation" placeholder="收纳点放置在？" placeholder-class="defaultPlaceHolderColor" value="" class="formControl"
        disabled/>
    </view>
    <view>
      <uploader user-infoa="{{userInfo}}"></uploader>
    </view>
    <view class="fn-mt-40">
      <button formType="submit" class="button thirdly">保存</button>
    </view>
  </form>
</view>
